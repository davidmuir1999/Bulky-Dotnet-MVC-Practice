@model List<Category> // retrieving the category list objected made, @model must be lower case

<h1>Category List</h1>
<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>
            Category Name
        </th>
        <th>
            Display Order
        </th>
    </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model){// using '@' can allow us to write #C code in .cshtml files
        //to fetch the @model previouly defined you must make it upper case
        <tr>
            <td>@obj.Name</td>
            <!-- accessing the categories data -->
            <td>@obj.DisplayOrder</td>
        </tr>
        }
    </tbody>
</table>